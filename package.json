{
  "name": "vue-plugin-remove-attributes",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@8.9.2",
  "description": "Vue plugin that removes attributes and directives from AST",
  "author": "Mihai Plamadeala <plamadeala.mihai002@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/lwvemike/vue-plugin-remove-attributes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lwvemike/vue-plugin-remove-attributes.git"
  },
  "bugs": "https://github.com/lwvemike/vue-plugin-remove-attributes/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@vue/compiler-core": "3.x",
    "vue": "3.x"
  },
  "optionalDependencies": {
    "@vue/compiler-sfc": "^3.3.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.22",
    "@antfu/ni": "^0.21.8",
    "@antfu/utils": "^0.7.6",
    "@types/node": "^20.8.3",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "esno": "^0.17.0",
    "happy-dom": "^12.9.1",
    "lint-staged": "^14.0.1",
    "pnpm": "^8.8.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": "eslint --fix",
    "src/*.ts": "tsc --noEmit"
  }
}
